package com.bmc.multithreading;

public class SingleThreadedApplication {
	
	public int sum(){
		int sum=0;
		for(int i=1;i<=10;i++){
			sum+=i;
		}
		
		return sum;
	}
	
	public int product(){
		
		int product=1;
		
		for(int i=1;i<=10;i++){
			product*=i;
		}
		return product;
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
