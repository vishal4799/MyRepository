package com.bmc.demo;

public class StringDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String str1="Hello";
		String str2="Hello";
		if(str1==str2){
			//Both reference variable point to same memory location
			//in String Pool
			System.out.println("str1==str2");
		}

	}

}
