package com.bmc.multithreaded;

public class MultithreadedApplication {

	/**
	 * @param args
	 */
	
	//Named Inner class
	
	class SumTask extends Thread{
		
		public double sum(){
			double sum=0;
			
			for(int i=1;i<=10;i++){
				sum+=i;
				System.out.println("Sum done so far:"+sum);
			}
			
			return sum;
		}
		
		@Override
		public void run(){
			
		}
		
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
